<table class = "table table-hover">
   <thead>
      <tr>
         <th> </th>
         <th>Name</th>
         <th>PDF</th>
         <th>Added</th>
      </tr>
   </thead>

   <tbody>
         <tr v-if="datasheets.length > 0" v-for="datasheet in datasheets">
            <td><i class="far fa-file-alt"></i></td>
            <td> {{datasheet.name}} </td>
            <td><a v-bind:href="datasheet.pdfDatasheet.url" > View PDF </a></td>
            <td> {{datasheet.updated_at}} </td>
         </tr>
   </tbody>

</table>
